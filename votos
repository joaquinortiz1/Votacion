class Candidate: 
    name=''
    political_party=''
    vote=''
    ##fin atributos##
    
    ##inicio metodos##
    
    def __init__(self,name,political_party,vote):
        self.name=name
        self.political_party=political_party
        self.vote=vote
        
    def 

    
    #def addition(self):
        #self.vote = vote + 1
        
names=[]
votes=[]
political_party_2=[]

size=2
        
for i in range(size):
    print("ingrese los datos de la persona",i + 1)
    name=input("nombre:")
    vote=input("voto:")
    political_party=input("partido politico:")
    
    names.append(name)
    votes.append(vote)
    political_party_2.append(political_party)
    
for i in range(size):
    print("mostrando los datos de la persona",i +1)
    print(i +1,")",names[i],political_party_2[i])
    print("voto por:",votes[i])
